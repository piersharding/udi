<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE template SYSTEM "template.dtd">
<template>
<title>Organisation Unit - UDI Template</title>
<regexp>^(dc|ou)=.*,</regexp>
<icon>ou.png</icon>
<description>New Organisation Unit - UDI Template</description>
<askcontainer>1</askcontainer>
<rdn>ou</rdn>
<visible>1</visible>

<objectClasses>
<objectClass id="organizationalUnit"></objectClass>
</objectClasses>

<attributes>
<attribute id="ou">
	<description>Organisation Unit</description>
	<display>Organisation Unit</display>
	<hint>don't include "ou="</hint>
	<order>1</order>
	<page>1</page>
</attribute>
<attribute id="description">
    <description>Organisation Description</description>
    <display>Organisation Description</display>
    <hint>description</hint>
    <order>2</order>
    <page>1</page>
</attribute>
</attributes>
</template>
